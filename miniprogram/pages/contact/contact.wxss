.container {
  height: 100%;
  background: #f2f2f2;
  font-size: 24rpx;
  color:#333;
}
.contact-title{
  margin-top: 50rpx; 
  font-size: 36rpx;
  padding-top: 30rpx;
}
.contact-icon-mail{
  width: 45rpx;
  height: 45rpx;
  line-height: 45rpx;
  margin-right: 10rpx;
}
.contact-icon{
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-right: 10rpx;
}
.contact-list{
  background: #fff; 
  margin-top: 20rpx; 
  padding: 20rpx 30rpx 10rpx 20px;
  /*border-top: 1px #f1f1f1 solid;*/
  /*border-bottom: 1px #c2c2c2 solid;*/
}
.contact-items{
  display: flex;
  flex: 1;
  margin-bottom: 15rpx;
  flex-direction: row;
  position: relative;
  font-size: 35rpx;
}
.head-items{
  display: flex;
  flex: 1;
  margin-bottom: 15rpx;
  flex-direction: row;
  position: relative;
  font-size: 30rpx;
  height: 100rpx;
}
.series{
  padding-top: 5rpx;
  font-size: 60rpx;
  font-weight: 700;
  position:absolute; 
  bottom:0;
}
.contact-type{
  color:black;
  padding-top: 5rpx;
  padding-left: 10rpx;
  font-size: 40rpx;
  right:0;
  position:absolute; 
  bottom:10rpx;
}
.font-type{
  color:black;
}
.font-type-1{
  position: absolute;
  right:0;
  text-align: right;
}
.channel{
  margin-right: 10rpx;
}
.earnest-money{
  color:#ec5c30;
}