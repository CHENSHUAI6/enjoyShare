<view class="container">
  <!-- <text class="contact-title">内容展示</text> -->
  <view class="contact-list"  wx:for="{{contactList}}" wx:key="{{index}}">
    <view class="head-items">
      <text class="series">{{item.name}}</text>
      <text class="contact-type">{{item.position}}</text>
    </view>
    <view class="contact-items">
      <image src="../../images/qq_2.png" mode="scollFill center" class="contact-icon"></image>
      <text bindtap="copyText" data-id="{{item.qq}}" class="font-type">{{item.qq}}</text>
    </view>
    <view class="contact-items">
      <image src="../../images/email_2.png" mode="scollFill center" class="contact-icon"></image>
      <text bindtap="copyText" data-id="{{item.email}}" class="font-type">{{item.email}}</text>
    </view>
    <view class="contact-items">
    <image src="../../images/wechat_2.png" mode="scollFill center" class="contact-icon"></image>
      <text bindtap="copyText" data-id="{{item.wechatName}}" class="font-type">{{item.wechatName}}</text>
    </view>
  </view>

